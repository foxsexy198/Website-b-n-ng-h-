			<option value="1">S5 Drop In No-MooMenu</option>
			<option value="2">Suckerfish</option>
			<option value="3">S5 Fading No-MooMenu</option>
			<option value="4">No Menu</option>
		</param>	
		<param name="xml_menuname" type="text" default="mainmenu" label="Menu Name" description="What menu would you like to use for the default default menu?" />	
		<param name="xml_s5_tooltips" type="list" default="yes" label="Enable Tooltips?" description="Would you like to enable tooltips? NOTE: It is highly recommened to disable this script if you are not planning to use it.">
			<option value="yes">yes</option>
			<option value="no">no</option>
		</param>
		<param name="xml_s5_lytebox" type="list" default="yes" label="Enable Lytebox?" description="Would you like to enable Lytebox? NOTE: Virtuemart uses this same script, so on all Virtuemart pages this script will automatically be disabled and the Virtuemart script will be used in its place on those pages only to avoid conflictions. It is highly recommened to disable this script if you are not planning to use it.">
			<option value="yes">yes</option>
			<option value="no">no</option>
		</param>	
		<param name="xml_s5_frontpage" type="list" default="no" label="Would You Like The Component and Article Area Shown On The Frontpage? - This will also disable the inset position." description="Choose whether or not to show the component and article section on the frontpage of the site.">
			<option value="yes">yes</option>
			<option value="no">no</option>
		</param>
		<param name="spacer2" type="spacer" default="If you publish any module to drop_down_1, drop_down_2 or drop_down_3 the S5 Drop Down will automatically appear. Below you can adjust the settings of the S5 Drop Down." />
		<param name="xml_s5_closed_text" type="text" default="The tab is closed" label="Closed Drop Down Text" description="If you publish any module to the advert1, advert2 or advert3 positions a drop down will automatically appear. What text would you like on the tab when it is closed?" />
		<param name="xml_s5_open_text" type="text" default="The tab is open" label="Open Drop Down Text" description="What text would you like to appear when it is open?" />
		<param name="xml_s5_user_speed" type="text" default="15" label="Drop Down Speed" description="What speed would you like the tab to open at? 1 is slow 30 is fast." />
		
	</params>
